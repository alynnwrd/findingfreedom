<div class = "container" id= "header">
  <form class="pure-form pure-form-aligned page1">
    <fieldset>
      <div class="pure-control-group">
        <label for="first-name"> First Name </label>
        <input type="text" id="first-name" placeholder="First Name" />
      </div>
      <div class="pure-control-group">
        <label for="last-name">Last Name</label>
        <input type="text" id="last-name" placeholder="Last Name" />
      </div>
        <div class="pure-control-group">
            <label for="email">Email Address</label>
            <input type="email" id="email" placeholder="Email Address" />
      </div>
      <div class="pure-control-group">
            <label for="phone-number">Phone Number</label>
            <input type="text" id="phone" placeholder="(000-000-0000)" />
      </div>
      <div class="pure-control-group">
        <label for="address">Street Address </label>
        <input type="text" id="multi-city" placeholder="Street Address" />
    </div>
      <div class="pure-control-group">
        <label for="multi-city">City</label>
        <input type="text" id="multi-city" placeholder="City" />
    </div>
    <div class="pure-control-group">
        <label for="state">State</label>
        <input type="text" id="city" placeholder="state">
    </div>
    <div class="pure-control-group">
        <label for="zip">Zip Code </label>
        <input type="text" id="zip" placeholder="Zip Code" />
     
     <button type="submit" id= "next-btn" class="pure-button pure-button-primary next-btn">Submit</button>
    </div>
  </fieldset>
</form>

</div>


